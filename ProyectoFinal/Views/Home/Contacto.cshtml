@model Contacto
@{
    ViewData["Title"] = "Contacto";
    ViewData["TitleHeader"] = "Contacto";
    ViewData["SubTitleHeader"] = "Envianos tus dudas y/o consultas";
    ViewData["imagen"] = "/assets/img/contact-bg.jpg";
}


<div class="container px-4 px-lg-5">
    <div class="row gx-4 gx-lg-5 justify-content-center">
        <div class="col-md-10 col-lg-8 col-xl-7">
            <p>Completa el formulario y envianos tus consultas. Te responderemos a la brevedad.</p>
            <div class="my-5">
                <form asp-action="Contacto">
                    <partial name="_FormContacto" model="@Model" />
                    <button type="submit" value="Contacto" class="btn btn-outline-primary text-uppercase">Enviar Consulta</button>
                </form>
            </div>
        </div>
    </div>
</div>